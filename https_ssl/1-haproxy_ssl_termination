global
    tune.ssl.default-dh-param 2048  # Moved to global section

defaults
    mode http
    timeout client 15s
    timeout connect 10s
    timeout server 15s
    timeout http-request 10s

frontend clickviral-tech-frontend
    bind *:80
    bind *:443 ssl crt /etc/letsencrypt/live/www.machine20learning.tech/machine20learning.pem
    default_backend clickviral-tech-backend

backend clickviral-tech-backend
    balance roundrobin
    server 6410-web-01 54.89.196.80:80 check
    server 6410-web-02 54.196.136.88:80 check
